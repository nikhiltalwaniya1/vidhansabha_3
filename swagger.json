        },
        "responses": {
          "200": {
            "description": "Create User",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "integer",
                      "description": "1 for success 0 for failure"
                    },
                    "message": {
                      "type": "string",
                      "description": "message to be displayed in alert"
                    },
                    "data": {
                      "type": "object"
                    }
                  }
                }
              }
            }
          },
          "default": {
            "description": "unexpected error",
            "content": {
